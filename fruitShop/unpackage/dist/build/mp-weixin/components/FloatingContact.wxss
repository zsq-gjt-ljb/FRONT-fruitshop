.floating-contact{position:fixed;z-index:999}.floating-contact .contact-btn{background-color:#fff;width:100rpx;height:100rpx;border-radius:50%;box-shadow:0 4rpx 20rpx rgba(0,0,0,.1);display:flex;flex-direction:column;align-items:center;justify-content:center;padding:0;margin:0;line-height:1}.floating-contact .contact-btn:after{border:none}.floating-contact .contact-btn.btn-hover{opacity:.8;transform:scale(.95)}.floating-contact .contact-btn .contact-icon{margin-bottom:6rpx}.floating-contact .contact-btn .contact-icon .icon-image{width:44rpx;height:44rpx}.floating-contact .contact-btn .contact-text{font-size:20rpx;color:#666;line-height:1}
